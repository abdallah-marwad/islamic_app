<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="400">
       <KeyFrameSet>
       </KeyFrameSet>
        <OnSwipe motion:touchAnchorId="@+id/azkarFrag_search"
            motion:springStiffness="40.0"
            motion:springMass="1.0"
            motion:springDamping="3.053"
            motion:maxAcceleration="0.1"
            motion:maxVelocity="0.1" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">

        <Constraint
            android:layout_height="@dimen/_33sdp"
            motion:layout_constraintBottom_toTopOf="parent"
            android:layout_width="match_parent"
            android:id="@+id/azkarFrag_search" />
    </ConstraintSet>
</MotionScene>